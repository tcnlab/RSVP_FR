	Journal-Full = {Hippocampus},
	Mesh = {Action Potentials; Animals; Exploratory Behavior; Hippocampus; Interneurons; Male; Motor Activity; Neural Pathways; Orientation; Pyramidal Cells; Rats; Rats, Inbred F344; Septal Nuclei; Space Perception; Synaptic Transmission; Theta Rhythm},
	Number = {7},

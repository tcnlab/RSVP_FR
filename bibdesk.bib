	Mesh = {Action Potentials; Animals; Electrodes, Implanted; Environment; Hippocampus; Male; Maze Learning; Memory; Microelectrodes; Motor Activity; Neurons; Rats; Rats, Long-Evans; Sleep; Space Perception; Wakefulness},
	Number = {7},

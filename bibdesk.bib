	Bdsk-Url-1 = {http://dx.doi.org/10.1101/lm.030296.113}}

@article{NiedEtal06,
	Abstract = {Humans and animals can nonverbally enumerate visual items across time in a sequence or rapidly estimate the set size of spatial dot patterns at a single glance. We found that temporal and spatial enumeration processes engaged different populations of neurons in the intraparietal sulcus of behaving monkeys. Once the enumeration process was completed, however, another neuronal population represented the cardinality of a set irrespective of whether it had been cued in a spatial layout or across time. These data suggest distinct neural processing stages for different numerical formats, but also a final convergence of the segregated information to form most abstract quantity representations.},

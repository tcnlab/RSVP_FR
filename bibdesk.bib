	Number = {19},
	Pages = {4596-604},
	Pmid = {15140931},
	Pst = {ppublish},
	Title = {Interaction between perirhinal and medial prefrontal cortex is required for temporal order but not recognition memory for objects in rats},
	Volume = {24},

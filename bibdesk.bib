	Doi = {10.1016/j.bandc.2015.11.003},
	Journal = {Brain Cogn},
	Journal-Full = {Brain and cognition},
	Keywords = {Cortex; Free energy; Neural networks; Predictive coding; Retina; Signal processing},
	Mesh = {Algorithms; Brain; Humans; Models, Neurological; Nerve Net; Neural Networks (Computer); Signal Processing, Computer-Assisted},
	Month = {03},

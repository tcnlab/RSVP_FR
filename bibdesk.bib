@article{WassEtal92,
	Annote = {Abstract:   Used a 3-step procedure to produce and disclose
nonsimilarity-based conceptualization in 8 pigeons. Merely by being associated
with the same response in the 1st step, classes of perceptually dissimilar
stimuli, like cars and chairs, appeared to amalgamate into a new category of
functionally equivalent stimuli. Thus, requiring a new response to be made to
only 1 of these 2 stimulus classes in the 2nd step transferred to the other
stimulus class in the 3rd step. This case of nonsimilarity-based
conceptualization in pigeons is relevant to the construction of superordinate
categories and to the role of language in secondary or mediated
generalization. (PsycINFO Database Record (c) 2004 APA, all rights reserved)},
	Author = {Wasserman, E. A. and DeVolder, C. L. and Coppage, D. J.},

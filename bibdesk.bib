@article{ArceEtal03,
	Annote = {Two experiments are reported that demonstrate temporal integration
of independently acquired temporal relationships, including backward
associations, in both human (Experiment 1) and nonhuman (rats, Experiment 2)
subjects. The experiments were designed and analyzed in the framework of the
temporal coding hypothesis (e.g., Matzel, Held, & Miller, 1988; Savastano &
Miller, 1998) as a strategy toward illuminating the use of temporal
information and assessing the existence of temporal backward associations.
Both experiments provided evidence of retrieval of associations to an event
that was expected to occur prior to the moment in time at which a stimulus was
presented (i.e., backward associations).  In addition, Experiment 1
constitutes the first controlled demonstration of temporal integration by
human subjects.},
	Author = {Arcediano, F. and Escobar, M. and Miller, R. R.},
	Journal = {Learning \& Behavior},
	Number = {3},
	Pages = {242-56},
	Title = {Temporal integration and temporal backward associations in human and nonhuman subjects.},
	Volume = {31},

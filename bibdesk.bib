	Mesh = {Brain Mapping; Female; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Mental Recall; Neuropsychological Tests; Oxygen; Photic Stimulation; Recognition (Psychology); Temporal Lobe},
	Number = {2},

	Mesh = {Animals; Macaca mulatta; Male; Neurons; Pattern Recognition, Visual; Photic Stimulation; Prefrontal Cortex; Psychomotor Performance; Reaction Time},
	Number = {22},
	Pages = {5986-93},
	Pmid = {17537970},
	Pst = {ppublish},
	Title = {A labeled-line code for small and large numerosities in the monkey prefrontal cortex},
	Volume = {27},

@article{KellEtal15,
	Abstract = {The two high threshold model (2HTM) of recognition memory makes strong predictions regarding differences between receiver operating characteristics (ROC) functions across strength manipulations. Province and Rouder (2012) tested these predictions and showed that the 2HTM provided a better account of the data than a continuous signal detection model using an extended two-alternative forced-choice task. The present study replicates and extends Province and Rouder's findings at the level of confidence-rating responses as well as their associated response times. Model-mimicry simulations are also reported, ascertaining that the models can be well discriminated in this experimental design. Supplemental files for this article are available at osf.io/zadt6/},
	Author = {David Kellen and Henrik Singmann and Jan Vogt and Karl Christoph Klauer},
	Date-Added = {2017-06-12 19:08:18 +0000},

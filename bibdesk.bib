@article{Dale76,
	Annote = {Conducted 2 experiments, with a total of 400 undergraduates, in which
output order was varied by recall instructions either during immediate and
delayed recall or during final recall. The serial position function varied
with changes in output order: regardless of input serial position, items
receiving early output priority had greater recall probabilities than, and

	Keywords = {intracranial EEG; memory; oscillations; reinstatement},
	Mesh = {Cerebral Cortex; Electroencephalography; Female; Humans; Male; Memory; Seizures; Synaptic Transmission},
	Number = {52},
	Pages = {18727-32},
	Pmc = {PMC4284613},
	Pmid = {25512550},
	Pst = {ppublish},
	Title = {Reinstatement of distributed cortical oscillations occurs with precise spatiotemporal dynamics during successful memory retrieval},
	Volume = {111},

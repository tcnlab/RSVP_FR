	Mesh = {Action Potentials; Animals; Cognition; Macaca mulatta; Mathematics; Memory; Models, Neurological; Nerve Net; Neurons; Neuropsychological Tests; Photic Stimulation; Prefrontal Cortex; Psychomotor Performance; Symbolism},

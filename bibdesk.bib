	Mesh = {Adult; Emotions; Factor Analysis, Statistical; Humans; Language; Memory; Models, Psychological},
	Number = {3},
	Pages = {423-9},
	Pmid = {17874582},
	Pst = {ppublish},
	Title = {"Remembering" emotional words is based on response bias, not recollection},
	Volume = {14},
	Year = {2007}}


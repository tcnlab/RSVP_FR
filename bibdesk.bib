	Bdsk-File-1 = {}}

@article{Rawl85,
	Annote = {Proposes that the hippocampus is needed to process stimuli of all
kinds, but only when there is a need to associate those stimuli with other
events that are temporally discontiguous. In order to form or use temporally
discontiguous associations, it is essential to maintain some memory of the 1st
component until the 2nd component has occurred. When the temporal gap to be
spanned is small, and the number of items to be temporarily retained is low, a
limited-capacity, short-term store is sufficient to allow associations to be
formed. Such a store is presumed to operate in parallel with the hippocampus
in normal animals. Hippocampal damage disrupts discontiguous associations but
is not thought to affect the long-term storage of associations of any kind, if
they can be formed. Analyses of research data show that by classifying tasks
in terms of the need to use a temporary memory store to retain temporally
discontiguous information, one can cut across existing classifications as well
as achieve a better fit to the data. The hippocampus thus seems best described
as a high-capacity, intermediate-term memory store. 18 comments on the article
and the author's reply are included. (6 p ref) (PsycINFO Database Record (c)
2009 APA, all rights reserved)},
	Author = {Rawlins, J. N.},
	Journal = {Behavioral and Brain Sciences},
	Pages = {479-528},
	Title = {Associations across time: {T}he hippocampus as a temporary memory store},
	Volume = {8},
	Year = {1985}}


	Mesh = {Animals; Calcium Channels, T-Type; Cell Line; Cerebral Cortex; Female; GTP-Binding Protein alpha Subunits, Gq-G11; Humans; Male; Membrane Potentials; Mice; Mice, Knockout; Phosphatidylinositol 4,5-Diphosphate; Phospholipase C beta; Pyramidal Cells; Rats; Receptors, Muscarinic; Signal Transduction; TRPC Cation Channels},
	Number = {32},

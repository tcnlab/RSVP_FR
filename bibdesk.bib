	Mesh = {Action Potentials; Animals; Conditioning (Psychology); Hippocampus; Male; Memory; Neural Pathways; Neurons; Pyramidal Cells; Rats; Rats, Wistar; Reaction Time; Synaptic Transmission; Time Factors},
	Number = {2},
	Pages = {685-98},
	Pmid = {16297564},
	Pst = {ppublish},
	Title = {Unambiguous representation of overlapping serial events in the rat hippocampal formation},
	Volume = {137},

	Journal = {Hippocampus},
	Journal-Full = {Hippocampus},
	Mesh = {Acoustic Stimulation; Animals; Brain Mapping; Conditioning, Classical; Corpus Callosum; Crosses, Genetic; Electroshock; Fear; Hippocampus; Motor Activity; Neocortex; Rats; Rats, Inbred BN; Rats, Inbred F344; Reaction Time; Reference Values},
	Number = {6},

	Isbn = {Simen, Patrick, Rivest, Francois , Ludvig, Elliot Andrew, 1975-, Balci , Fuat and Killeen, Peter. (2013) Timescale invariance in the pacemaker-accumulator family of timing models. Timing {\&} Time Perception, Volume 1 (Number 2). pp. 159-188. ISSN 2213-445X},
	Issn = {2213-445X},
	Journal = {Timing {\&} Time Perception},
	Keywords = {BF Psychology,RC0321 Neuroscience. Biological psychiatry. Neurop},

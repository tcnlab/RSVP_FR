	Number = {1},
	Pages = {253-69},
	Pmc = {PMC3683557},
	Pmid = {22924326},
	Pst = {ppublish},
	Title = {Memory asymmetry of forward and backward associations in recognition tasks},
	Volume = {39},
	Year = {2013},

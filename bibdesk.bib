@article{SidiEtal09,
	Abstract = {The intrinsic properties of neurons that enable them to maintain depolarized, persistently activated states in the absence of sustained input are poorly understood. In short-term memory tasks, individual prefrontal cortical (PFC) neurons can maintain persistent action potential output during delay periods between informative cues and behavioral responses. Dopamine and drugs of abuse alter PFC function and working memory, possibly by modulating intrinsic neuronal properties. Here we used patch-clamp recording of layer 5 PFC pyramidal neurons to identify a postsynaptic depolarization that was evoked by action potential bursts and mediated by metabotropic glutamate receptor 5 (mGluR5). This depolarization occurred in the absence of recurrent synaptic activity and was reduced by a dopamine D1 receptor (D1R) protein kinase A pathway. After behavioral sensitization to cocaine, the depolarization was substantially diminished and D1R modulation was lost. We propose that burst-evoked intrinsic depolarization is a form of short-term cellular memory that is modulated by dopamine and cocaine experience.},
	Author = {Sidiropoulou, Kyriaki and Lu, Fang-Min and Fowler, Melissa A and Xiao, Rui and Phillips, Christopher and Ozkan, Emin D and Zhu, Michael X and White, Francis J and Cooper, Donald C},

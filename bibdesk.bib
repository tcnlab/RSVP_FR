@article{VissKapl07,
	Abstract = {Are the information processing steps that support short-term sensory memory common to all the senses? Systematic, psychophysical comparison requires identical experimental paradigms and comparable stimuli, which can be challenging to obtain across modalities. Participants performed a recognition memory task with auditory and visual stimuli that were comparable in complexity and in their neural representations at early stages of cortical processing. The visual stimuli were static and moving Gaussian-windowed, oriented, sinusoidal gratings (Gabor patches); the auditory stimuli were broadband sounds whose frequency content varied sinusoidally over time (moving ripples). Parallel effects on recognition memory were seen for number of items to be remembered, retention interval, and serial position. Further, regardless of modality, predicting an item's recognizability requires taking account of (1) the probe's similarity to the remembered list items (summed similarity), and (2) the similarity between the items in memory (inter-item homogeneity). A model incorporating both these factors gives a good fit to recognition memory data for auditory as well as visual stimuli. In addition, we present the first demonstration of the orthogonality of summed similarity and inter-item homogeneity effects. These data imply that auditory and visual representations undergo very similar transformations while they are encoded and retrieved from memory.},
	Author = {Visscher, Kristina M and Kaplan, Elina and Kahana, Michael J and Sekuler, Robert},
	Date-Added = {2013-01-11 00:55:57 +0000},
	Date-Modified = {2018-09-13 08:22:22 +0000},
	Doi = {10.1371/journal.pbio.0050056},
	Journal = {PLoS Biol},
	Journal-Full = {PLoS biology},
	Mesh = {Adolescent; Adult; Hearing; Humans; Memory; Vision, Ocular},
	Number = {3},
	Pages = {e56},
	Pmc = {PMC1800308},
	Pmid = {17311472},
	Pst = {ppublish},
	Title = {Auditory short-term memory behaves like visual short-term memory},
	Volume = {5},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371/journal.pbio.0050056}}

@article{ZhouKaha04,
	Abstract = {Cognition is shaped by the way that past experiences are represented in memory. To examine the representation of recent visual experiences, we devised a novel procedure that measures episodic recognition memory for synthetic textures. On each trial, two brief study stimuli were followed by a probe, which either replicated one of the study stimuli or differed in spatial frequency from both. The probe's spatial frequency roved from trial to trial, testing recognition with a range of differences between probe and study items. Repeated testing of recognition generated mnemometric functions, snapshots of memory strength's distribution. The distributional characteristics of the mnemometric functions rule out several hypotheses about memory representation, including the hypothesis that representations are prototypes constructed from previously seen stimuli; instead, stimuli are represented in memory as noisy exemplars.},
	Author = {Zhou, Feng and Kahana, Michael J and Sekuler, Robert},

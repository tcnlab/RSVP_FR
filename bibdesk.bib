	Journal = {Frontiers in Human Neuroscience},
	Journal-Full = {Frontiers in human neuroscience},
	Keywords = {CDA; ERPs; N2PC; N3RS; attention; memory; retrieval; search},
	Pages = {4},
	Pmc = {PMC2525979},
	Pmid = {18958218},
	Pst = {epublish},
	Title = {Spatial attention can bias search in visual short-term memory},

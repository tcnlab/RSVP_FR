	Journal-Full = {The European journal of neuroscience},
	Mesh = {Adolescent; Adult; Corpus Striatum; Female; Forecasting; Hippocampus; Humans; Learning; Male; Photic Stimulation; Psychomotor Performance; Reaction Time; Young Adult},
	Number = {7},
	Pages = {1011-23},
	Pmc = {PMC3325519},
	Pmid = {22487032},
	Pst = {ppublish},
	Title = {Dissociating hippocampal and striatal contributions to sequential prediction learning},
	Volume = {35},

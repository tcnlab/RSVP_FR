	Journal = {Journal of Neuroscience},
	Journal-Full = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Number = {7},
	Pages = {2473-84},
	Pmid = {22396421},
	Pst = {ppublish},
	Title = {Temporal convergence of dynamic cell assemblies in the striato-pallidal network},
	Volume = {32},
	Year = {2012},
